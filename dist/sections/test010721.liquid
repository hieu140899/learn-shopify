<div class="pageOverlay"></div>
<div class="pageOverlay2"></div>
<div class="pageOverlay2"></div>

<div class="Section-Auguste">
  <div class="Auguste-container">
      <div class="Auguste-header">
          <div class="header-subtitle">
              Welcome To
          </div>
          <div class="header-title">
            {{ section.settings.title1 }}
          </div>
          <div class="header-desc">
              {{ section.settings.text1 }}
          </div>
      </div>
      <div class="Auguste-content1">
          <div class="Auguste-content1-imagebox">
              <div class="image1">
                {%- assign block_image = section.settings.image1 -%}
                {% if block_image == blank %}
                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                {% else %}
                    <img src="{{ section.settings.image1 | img_url: 'master' }}"
                        data-src="{{ section.settings.image1 | img_url: '1000x1000' }}" alt="" class="img1">
                {% endif %}
              </div>
              <div class="image2">
                {%- assign block_image2 = section.settings.image2 -%}
                {% if block_image2 == blank %}
                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                {% else %}
                    <img src="{{ section.settings.image2 | img_url: 'master' }}"
                        data-src="{{ section.settings.image2 | img_url: '1000x1000' }}" alt="" class="img1">
                {% endif %}
              </div>
          </div>
          <div class="Auguste-content1-choosebox">
              <div class="choosebox-productname">
                  {{ section.settings.productName }}
              </div>
              <div class="choosebox-price">
                  {{ section.settings.price }}
              </div>
              <div class="choosebox-pickcolor">
                  <div class="colorname">
                      <div class="color-name1">Color: {{ section.settings.color1 }} </div>
                      <div class="color-name2">Color: {{ section.settings.color2 }} </div>
                  </div>
                  <div class="colorpreview">
                      <div class="color-item item1 active-border">
                        {%- assign block_image = section.settings.image1 -%}
                        {% if block_image == blank %}
                            {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                        {% else %}
                            <img src="{{ section.settings.image1 | img_url: 'master' }}"
                                data-src="{{ section.settings.image1 | img_url: '1000x1000' }}" alt="" class="img1">
                        {% endif %}
                      </div>
                      <div class="color-item item2">
                        {%- assign block_image2 = section.settings.image2 -%}
                        {% if block_image2 == blank %}
                            {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                        {% else %}
                            <img src="{{ section.settings.image2 | img_url: 'master' }}"
                                data-src="{{ section.settings.image2 | img_url: '1000x1000' }}" alt="" class="img1">
                        {% endif %}
                      </div>
                  </div>
              </div>
              <div class="choosebox-sizepicker">
                  <div class="label">
                      Size:
                      <div class="chart-btn">Size chart</div>
                  </div>



                  <div class="size-table">
                      <div class="size-box">
                          <div class="size-box-header">
                              <span class="size-box-header-btn">
                                  <svg class="Icon Icon--close" role="presentation" viewBox="0 0 16 14" style="height: 16px; width: 16px;">
                                      <path d="M15 0L1 14m14 0L1 0" stroke="currentColor" fill="none" fill-rule="evenodd"></path>
                                  </svg>
                              </span>
                              <span class="size-box-header-label">Size</span>
                          </div>
                          <ul class="size-box-option">
                              <li><a href="" class="option-item">XS</a></li>
                              <li><a href="" class="option-item">S</a></li>
                              <li><a href="" class="option-item">M</a></li>
                              <li><a href="" class="option-item">L</a></li>
                          </ul>
                      </div>

                      <div class="pick-btn">
                          <span>XS</span>
                          <span class="pick-icon">
                              <svg class="Icon Icon--select-arrow" role="presentation" viewBox="0 0 19 12" style="width: 12px; height: 12px;">
                                  <polyline fill="none" stroke="currentColor" points="17 2 9.5 10 2 2" fill-rule="evenodd" stroke-width="2" stroke-linecap="square"></polyline>
                              </svg>
                          </span>
                      </div>
                  </div>
                  <div class="textsub">Only 2 pieces in stock!</div>
              </div>
              <div class="choosebox-btn">
                  <div>
                      <a href="" class="augustebtn btn-add">ADD TO CART</a>
                  </div>
                  <div>
                      <a href="" class="augustebtn btn-buy">BUY IT NOW</a>
                  </div>
                  <a href="" class="btn-details">View product details</a>
              </div>
          </div>
      </div>
  </div>
</div>

